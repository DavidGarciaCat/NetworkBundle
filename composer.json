  {
  "name": "davidgarciacat/network-polyfill",
  "description": "Set of reusable network tools, based on PEAR PHP extensions, but developed using standard PHP.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "David Garcia",
      "email": "me@davidgarcia.cat"
    }
  ],
  "autoload": {
    "psr-4": {
      "DavidGarciaCat\\NetworkPolyfill\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\DavidGarciaCat\\NetworkPolyfill\\": "spec/"
    }
  },
  "require": {
    "php": ">=5.3"
  },
  "require-dev": {
    "phpspec/phpspec": "^2.5",
    "henrikbjorn/phpspec-code-coverage": "^2.1"
  },
  "config": {
    "bin-dir": "bin"
  },
  "minimum-stability": "stable"
}
